<?xml version="1.0" encoding="utf-8" ?>
<Project>
	<Target Name="MovetoPlugins" AfterTargets="Build" Condition="'$(CI)'!='TRUE'">
		<Copy SourceFiles="$(TargetPath)" DestinationFiles="$(PluginsPath)\$(TargetFileName)" />
		<Message Text="Move DLL to Plugins" Importance="high"/>
	</Target>
</Project>
